{
    "_version": "1.59.0",
    "sap.app": {
        "id": "hr.axians.zeptd",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "dataSources": {
            "ZEPTD_SRV_BIND1": {
                "uri": "/sap/opu/odata/sap/ZEPTD_SRV_BIND",
                "type": "OData",
                "settings": {
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "E-PTD-generate": {
                    "semanticObject": "E_PTD",
                    "action": "generate",
                    "title": "{{flpTitle}}",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://detail-view",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "hr.axians.zeptd.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.125.0",
            "libs": {
                "sap.f": {},
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "hr.axians.zeptd.i18n.i18n"
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "ZEPTD_SRV_BIND1",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "hr.axians.zeptd.view",
                "controlId": "layout",
                "controlAggregation": "beginColumnPages",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "master",
                    "target": "master"
                },
                {
                    "pattern": "EPTD/{objectId}",
                    "name": "eptddetails",
                    "target": [
                        "master",
                        "eptddetails"
                    ]
                }
            ],
            "targets": {
                "master": {
                    "name": "Master",
                    "id": "master",
                    "level": 1
                },
                "eptddetails": {
                    "name": "Detail",
                    "id": "eptddetails",
                    "level": 1,
                    "controlAggregation": "midColumnPages"
                },
                "notFound": {
                    "name": "NotFound",
                    "id": "notFound"
                },
                "detailObjectNotFound": {
                    "name": "DetailObjectNotFound",
                    "id": "detailObjectNotFound",
                    "controlAggregation": "midColumnPages"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}